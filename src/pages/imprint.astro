---
import PageLayout from "../layouts/page-layout.astro";

const url = "https://imprint.acdh.oeaw.ac.at/21818";
const response = await fetch(url);
const data = await response.text();
---

<PageLayout>
	<div class="mx-auto w-full max-w-screen-xl px-16">
		<div class="prose max-w-screen-md">
			<h1>Imprint</h1>
			<div set:html={data} />
		</div>
	</div>
</PageLayout>
